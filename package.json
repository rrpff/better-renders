  "scripts": {
    "prepublish": "yarn run build",
    "lint": "eslint -c .eslintrc .",
    "test-client": "cd chemist-client && yarn test",
    "test-render": "cd chemist-render && yarn test",
    "test-mktemplate": "cd mktemplate && yarn test",
    "test-chemist": "yarn run build-chemist && NODE_ENV=test mocha test/lib/*.js test/cli/*.js",
    "test": "concurrently --raw \"yarn run test-client\" \"yarn run test-render\" \"yarn run test-mktemplate\" \"yarn run test-chemist\"",
    "build-client": "cd chemist-client && yarn build",
    "build-render": "cd chemist-render && yarn build",
    "build-mktemplate": "cd mktemplate && yarn build",
    "build-chemist": "babel src --out-dir dist && chmod u+x dist/cli/index.js",
    "build": "concurrently \"yarn run build-client\" \"yarn run build-render\" \"yarn run build-mktemplate\" \"yarn run build-chemist\""
  },
